{
  "typescript.tsdk": "node_modules/typescript/lib",
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.formatOnSave": false,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "cSpell.words": ["gorm", "unocss", "mapstructure", "vuetify"],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    ".eslintrc.json",
    "*.config.ts",
    "*-lock.yaml"
  ]
}
